{
	"name": "checkout-engine/input",
	"category": "layout",
	"supports": {
		"reusable": false
	},
	"attributes": {
		"value": {
			"type": "string",
			"default": ""
		},
		"placeholder": {
			"type": "string",
			"default": ""
		},
		"disabled": {
			"type": "boolean",
			"default": true
		},
		"size": {
			"type": "string",
			"default": "large"
		},
		"label": {
			"type": "string"
		},
		"help": {
			"type": "string"
		},
		"inputmode": {
			"type": "string"
		},
		"max": {
			"type": [ "string", "number" ]
		},
		"maxlength": {
			"type": "number"
		}
	}
}
