{
	"name": "checkout-engine/form",
	"title": "Checkout Engine Form",
	"description": "Displays a form.",
	"category": "layout",
	"parent": ["checkout-engine/checkout-form"],
	"keywords": ["email", "mail"],
	"supports": {
		"reusable": false,
		"html": false,
		"color": {
			"background": true,
			"link": true,
			"text": true
		},
		"spacing": {
			"blockGap": "25px",
			"margin": ["top", "bottom"],
			"padding": true,
			"__experimentalDefaultControls": {
				"blockGap": true
			}
		}
	},
	"attributes": {
		"mode": {
			"type": "string",
			"enum": ["test", "live"],
			"default": "live"
		},
		"prices": {
			"type": "array"
		}
	}
}
