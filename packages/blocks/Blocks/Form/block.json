{
	"name": "surecart/form",
	"title": "SureCart Form",
	"description": "Displays a form.",
	"category": "layout",
	"parent": ["surecart/checkout-form"],
	"keywords": ["email", "mail"],
	"supports": {
		"reusable": false,
		"html": false
	},
	"attributes": {
		"mode": {
			"type": "string",
			"enum": ["test", "live"],
			"default": "live"
		},
		"prices": {
			"type": "array"
		},
		"gap": {
			"type": "string",
			"default": "25px"
		},
		"redirect": {
			"type": "string",
			"default": ""
		}
	}
}
