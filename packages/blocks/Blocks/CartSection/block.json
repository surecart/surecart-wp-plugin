{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "surecart/cart-section",
	"title": "Cart Section",
	"description": "A cart section",
	"category": "surecart",
	"ancestor": ["surecart/cart"],
	"supports": {
		"inserter": false,
		"reusable": false,
		"html": false
	},
	"providesContext": {
		"surecart/slot": "slot"
	},
	"attributes": {
		"slot": {
			"type": "string",
			"default": "footer"
		}
	}
}
