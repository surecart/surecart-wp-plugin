{
	"apiVersion": 2,
	"name": "surecart/product-collections",
	"title": "Product Collections",
	"description": "Displays product collection selector for a product.",
	"category": "surecart",
	"keywords": ["collection", "form", "product"],
	"attributes": {
		"size": {
			"type": "string",
			"default": "small"
		},
		"pill": {
			"type": "boolean",
			"default": false
		},
		"type": {
			"type": "string",
			"default": "primary"
		},
		"collectionCount": {
			"type": "number",
			"default": 100
		}
	},
	"supports": {
		"reusable": false,
		"html": false,
		"multiple": false
	}
}
