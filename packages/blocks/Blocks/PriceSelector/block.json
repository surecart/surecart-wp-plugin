{
	"apiVersion": 2,
	"name": "surecart/price-selector",
	"title": "Price Selector",
	"description": "Displays a selector for product prices.",
	"category": "layout",
	"keywords": ["price", "product", "choice", "checkout"],
	"parent": ["surecart/form", "surecart/column"],
	"supports": {
		"reusable": false,
		"html": false
	},
	"attributes": {
		"label": {
			"type": "string"
		},
		"choices": {
			"type": "array",
			"default": []
		},
		"type": {
			"type": "string",
			"default": "radio"
		},
		"columns": {
			"type": "number",
			"default": 1
		}
	}
}
