{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "surecart/product-list",
	"title": "Product List",
	"description": "Display a list of products from your store.",
	"category": "surecart",
	"keywords": ["products", "product", "list", "store"],
	"supports": {
		"reusable": false,
		"html": false
	},
	"attributes": {
		"rows": {
			"type": "number",
			"default": 4
		},
		"columns": {
			"type": "number",
			"default": 4
		},
		"gap": {
			"type": "string",
			"default": "1rem"
		}
	}
}
