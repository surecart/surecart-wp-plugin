{
	"name": "surecart/store-logo",
	"title": "Store Logo",
	"description": "Display the store logo.",
	"category": "surecart",
	"keywords": ["logo", "image", "store", "store logo"],
	"supports": {
		"html": false,
		"spacing": {
			"margin": true,
			"padding": true
		}
	},
	"attributes": {
		"width": {
			"type": "number"
		},
		"height": {
			"type": "number"
		},
		"maxWidth": {
			"type": "number",
			"default": 180
		},
		"maxHeight": {
			"type": "number",
			"default": 80
		},
		"isLinkToHome": {
			"type": "boolean",
			"default": true
		}
	}
}
