{
  "name": "checkout-engine/input",
	"title": "Input",
  "description": "Displays a form text input.",
	"category": "layout",
  "keywords": ["email", "mail"],
  "supports": {
		"reusable": false,
		"html": false
	},
	"attributes": {
		"value": {
			"type": "string",
			"default": ""
		},
		"placeholder": {
			"type": "string",
			"default": ""
		},
		"disabled": {
			"type": "boolean",
			"default": false
		},
		"size": {
			"type": "string",
			"default": ""
		},
		"label": {
			"type": "string"
		},
		"help": {
			"type": "string"
		},
		"inputmode": {
			"type": "string"
		},
		"max": {
			"type": [ "string", "number" ]
		},
		"maxlength": {
			"type": "number"
		},
    "name": {
      "type": "string"
    }
	}
}
