{
	"name": "@checkout-engine/components-react",
	"sideEffects": false,
	"version": "0.0.1",
	"private": true,
	"description": "React specific wrapper for checkout-engine/components",
	"repository": {
		"type": "git",
		"url": "https://github.com/ionic-team/ionic.git"
	},
	"scripts": {
		"build": "npm run clean && npm run compile",
		"watch": "npm run build",
		"clean": "rm -rf dist",
		"compile": "npm run tsc",
		"tsc": "tsc -p .",
		"rollup": "rollup -c"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.es.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist/"
	],
	"devDependencies": {
		"@types/jest": "23.3.9",
		"@types/node": "^15.12.2",
		"@types/react": "^17.0.34",
		"@types/react-dom": "^17.0.11",
		"jest": "^23.0.0",
		"jest-dom": "^3.0.2",
		"np": "^3.1.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"typescript": "^3.3.4000"
	},
	"peerDependencies": {
		"react": "^16.7.0",
		"react-dom": "^16.7.0"
	},
	"jest": {
		"preset": "ts-jest",
		"setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
		"testPathIgnorePatterns": [
			"node_modules",
			"dist"
		]
	}
}
