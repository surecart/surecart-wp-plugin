{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "surecart/product-template",
	"keywords": ["product", "list"],
	"title": "Template",
	"description": "Displays products list.",
	"version": "1.0.0",
	"category": "surecart",
	"icon": "media-interactive",
	"example": {
		"attributes": {}
	},
	"allowedBlocks": [
		"surecart/product-image",
		"surecart/product-name",
		"surecart/product-price-v2"
	],
	"ancestor": [ "surecart/product-list" ],
	"usesContext": [
		"surecart/product-list/blockId",
		"surecart/product-list/columns",
		"surecart/product-list/limit"
	],
	"supports": {
		"interactivity": {
			"clientNavigation": true
		},
		"spacing": {
			"padding": true,
			"margin": true,
			"blockGap": true,
			"__experimentalDefault": "2em",
			"__experimentalDefaultControls": {
				"padding": true,
				"margin": true,
				"blockGap": true
			}
		},
		"color": {
			"text": false,
			"background": true
		},
		"__experimentalBorder": {
			"color": true,
			"radius": true,
			"width": true,
			"__experimentalDefaultControls": {
				"color": true,
				"radius": true,
				"width": true
			}
		}
	},
	"textdomain": "surecart",
	"editorScript": "file:./index.js",
	"viewScriptModule": "@surecart/product-list",
	"style": [
		"file:../../styles/product-item-list.css",
		"file:../../styles/product-item.css"
	]
}
