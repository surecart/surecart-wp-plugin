{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "surecart/product-page",
	"title": "Product Page",
	"description": "Display product page information.",
	"version": "1.0.0",
	"category": "surecart-product-page",
	"providesContext": {
		"surecart/product_id": "product_id"
	},
	"attributes": {
		"product_id": {
			"type": "string"
		}
	},
	"supports": {
		"interactivity": true,
		"align": ["wide", "full"]
	},
	"textdomain": "surecart",
	"render": "file:./view.php",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./editor.css",
	"viewScript": ["wp-url", "wp-api-fetch"],
	"viewScriptModule": ["@surecart/product-page"]
}
