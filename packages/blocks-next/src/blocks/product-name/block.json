{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "surecart/product-name",
	"title": "Name",
	"description": "Display the product name.",
	"version": "1.0.0",
	"category": "surecart",
	"icon": "media-interactive",
	"example": {},
	"usesContext": ["surecart/productId"],
	"ancestor": ["surecart/product-list"],
	"attributes": {
		"textAlign": {
			"type": "string",
			"default": "left"
		},
		"level": {
			"type": "number",
			"default": 1
		}
	},
	"supports": {
		"interactivity": {
			"clientNavigation": true
		},
		"spacing": {
			"padding": true,
			"margin": ["top", "bottom"]
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"__experimentalFontWeight": true
		},
		"color": {
			"text": true,
			"background": false,
			"enableContrastChecker": true
		}
	},
	"render": "file:./view.php",
	"textdomain": "surecart",
	"editorScript": "file:./index.js",
	"viewScriptModule": "@surecart/product-page",
	"style": ["file:../../styles/product-item-title.css"]
}
