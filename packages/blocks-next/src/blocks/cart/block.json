{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "surecart/cart-v2",
	"title": "SureCart Cart v2",
	"description": "Display SureCart Cart",
	"version": "1.0.0",
	"category": "cart",
	"icon": "media-interactive",
	"example": {},
	"providesContext": {
		"surecart/cart-v2/blockId": "cart-v2"
	},
	"usesContext": ["surecart/cart-v2/blockId"],
	"attributes": {
		"title": {
			"type": "string",
			"default": "Cart"
		},
		"width": {
			"type": "string",
			"default": "500px"
		}
	},
	"supports": {
		"interactivity": true,
		"layout": true,
		"html": false,
		"multiple": false
	},
	"render": "file:./view.php",
	"textdomain": "surecart",
	"editorScript": "file:./index.js",
	"style": [
		"file:../../styles/drawer.css",
		"file:../../styles/block-ui.css",
		"file:./index.css"
	],
	"viewScript": ["wp-url", "wp-api-fetch"],
	"viewScriptModule": ["@surecart/checkout", "@surecart/dialog"]
}
