{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "surecart/product-list-filter-tag",
	"title": "Tag",
	"description": "Display the filter tag.",
	"version": "1.0.0",
	"category": "surecart-product-list",
	"example": {},
	"usesContext": ["surecart/filterTag/name"],
	"ancestor": ["surecart/product-list-filter-tags-template"],
	"supports": {
		"interactivity": true,
		"spacing": {
			"padding": true,
			"margin": ["top", "bottom"]
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"__experimentalFontWeight": true
		},
		"color": {
			"text": true,
			"background": true,
			"enableContrastChecker": true
		},
		"__experimentalBorder": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true,
			"__experimentalDefaultControls": {
				"color": true,
				"radius": true,
				"style": true,
				"width": true
			}
		}
	},
	"textdomain": "surecart",
	"editorScript": "file:./index.js",
	"style": ["surecart-checkbox"],
	"render": "file:./view.php"
}
