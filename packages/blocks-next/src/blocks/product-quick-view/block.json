{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "surecart/product-quick-view",
	"title": "Product Quick View",
	"description": "Display the product quick view.",
	"version": "1.0.0",
	"category": "surecart-product-list",
	"example": {},
	"usesContext": ["postId"],
	"ancestor": ["surecart/product-template"],
	"attributes": {
		"icon": {
			"type": "string",
			"default": "eye"
		}
	},
	"supports": {
		"interactivity": true,
		"color": {
			"gradients": true,
			"link": false,
			"__experimentalDefaultControls": {
				"background": true,
				"text": true
			}
		},
		"spacing": {
			"margin": true,
			"padding": true
		}
	},
	"textdomain": "surecart",
	"render": "file:./view.php",
	"style": "file:./style-index.css",
	"editorScript": "file:./index.js",
	"viewScriptModule": ["@surecart/checkout", "@surecart/product-page"]
}
