{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "surecart/product-price-chooser",
	"title": "Product Prices",
	"description": "Displays possible choices for product prices.",
	"version": "1.0.0",
	"category": "surecart-product-page",
	"ancestor": ["surecart/product-page"],
	"supports": {
		"interactivity": true,
		"spacing": {
			"padding": true,
			"margin": false,
			"__experimentalDefaultControls": {
				"padding": true
			}
		},
		"__experimentalBorder": {
			"color": true,
			"radius": true,
			"width": true,
			"__experimentalDefaultControls": {
				"color": true,
				"radius": true,
				"width": true
			}
		},
		"color": {
			"gradients": true,
			"text": true,
			"background": true,
			"__experimentalDefaultControls": {
				"text": true,
				"background": true
			}
		}
	},
	"attributes": {
		"label": {
			"type": "string",
			"default": "Pricing"
		}
	},
	"textdomain": "surecart",
	"editorScript": "file:./index.js",
	"render": "file:./view.php",
	"style": [
		"file:../../styles/choice.css",
		"file:../../styles/label.css",
		"file:./style-index.css"
	]
}
