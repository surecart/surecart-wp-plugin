{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "surecart/product-list",
	"title": "Product List",
	"description": "Display a list of products.",
	"version": "1.0.0",
	"category": "surecart-product-list",
	"example": {},
	"providesContext": {
		"query": "query",
		"surecart/product-list/limit": "limit",
		"surecart/product-list/offset": "offset",
		"surecart/product-list/type": "type",
		"surecart/product-list/ids": "ids"
	},
	"attributes": {
		"ids": {
			"type": "array",
			"default": []
		},
		"type": {
			"type": "string",
			"default": "all"
		},
		"limit": {
			"type": "number"
		},
		"collection_id": {
			"type": "string",
			"default": ""
		},
		"query": {
			"type": "object",
			"default": {
				"perPage": 15,
				"pages": 0,
				"offset": 0,
				"postType": "sc_product",
				"order": "desc",
				"orderBy": "date",
				"author": "",
				"search": "",
				"exclude": [],
				"include": [],
				"sticky": "",
				"inherit": true,
				"taxQuery": null,
				"parents": []
			}
		}
	},
	"supports": {
		"interactivity": true,
		"layout": true,
		"align": true,
		"spacing": {
			"padding": true,
			"margin": true,
			"blockGap": true
		}
	},
	"textdomain": "surecart",
	"editorScript": "file:./index.js",
	"style": [
		"file:./style-index.css",
		"file:../../styles/block-ui.css",
		"file:../../styles/tag.css",
		"surecart-theme-base"
	]
}
