{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "surecart/product-list",
	"keywords": ["product", "list", "raj"],
	"title": "New Product List",
	"description": "Displays products list.",
	"version": "1.0.0",
	"category": "surecart",
	"icon": "media-interactive",
	"example": {
		"attributes": {}
	},
	"attributes": {
		"product_id": {
			"type": "string"
		},
		"columns": {
			"type": "number",
			"default": 4
		},
		"limit": {
			"type": "number",
			"default": 15
		}
	},
	"supports": {
		"interactivity": true,
		"spacing": {
			"blockGap": true,
			"__experimentalDefaultControls": {
				"blockGap": true
			}
		}
	},
	"textdomain": "surecart",
	"editorScript": "file:./index.js",
	"viewScriptModule": "@surecart/product-list",
	"style": ["file:../../styles/product-item-list.css", "file:../../styles/product-item.css"]
}
