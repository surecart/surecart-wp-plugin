{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "surecart/product-list",
	"title": "Product List",
	"description": "Display the products.",
	"version": "1.0.0",
	"category": "surecart",
	"icon": "media-interactive",
	"example": {},
	"providesContext": {
		"surecart/product-list/blockId": "blockId",
		"surecart/product-list/columns": "columns",
		"surecart/product-list/limit": "limit"

	},
	"attributes": {
		"blockId": {
			"type": "string",
			"default": ""
		},
		"columns": {
			"type": "number",
			"default": 4
		},
		"limit": {
			"type": "number",
			"default": 15
		}
	},
	"allowedBlocks": [
		"surecart/product-template",
		"surecart/product-pagination"
	],
	"supports": {
		"interactivity": {
			"clientNavigation": true
		}
	},
	"textdomain": "surecart",
	"editorScript": "file:./index.js",
	"viewScriptModule": "@surecart/product-page"
}
