{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "surecart/product-item-v2",
	"title": "Product Item",
	"description": "Display the product item.",
	"version": "1.0.0",
	"category": "surecart",
	"icon": "media-interactive",
	"example": {},
	"usesContext": ["surecart/productId"],
	"ancestor": [ "surecart/product-list" ],
	"attributes": {},
	"supports": {
		"interactivity": {
			"clientNavigation": true
		},
		"reusable": false,
		"html": false,
		"spacing": {
			"padding": true,
			"margin": true,
			"__experimentalDefault": "2em",
			"__experimentalDefaultControls": {
				"padding": true,
				"margin": true
			}
		},
		"color": {
			"text": false,
			"background": true
		},
		"__experimentalBorder": {
			"color": true,
			"radius": true,
			"width": true,
			"__experimentalDefaultControls": {
				"color": true,
				"radius": true,
				"width": true
			}
		}
	},
	"textdomain": "surecart",
	"viewScriptModule": "@surecart/product-page",
	"editorScript": "file:./index.js"
}
