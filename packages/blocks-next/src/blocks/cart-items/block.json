{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 3,
	"name": "surecart/cart-items-v2",
	"title": "Cart Items",
	"description": "The cart items",
	"category": "cart",
	"version": "1.0.0",
	"example": {},
	"textdomain": "surecart",
	"ancestor": ["surecart/cart-v2"],
	"attributes": {
		"removable": {
			"type": "boolean",
			"default": true
		},
		"editable": {
			"type": "boolean",
			"default": true
		},
		"border": {
			"type": "boolean",
			"default": true
		},
		"padding": {
			"type": "object",
			"default": {
				"top": "1.25em",
				"left": "1.25em",
				"bottom": "1.25em",
				"right": "1.25em"
			}
		},
		"backgroundColor": {
			"type": "string"
		},
		"textColor": {
			"type": "string"
		}
	},
	"supports": {
		"interactivity": true,
		"html": false,
		"multiple": false
	},
	"render": "file:./view.php",
	"editorScript": "file:./index.js",
	"style": [
		"file:../../styles/line-item.css",
		"file:../../styles/product-line-item.css",
		"file:../../styles/input-group.css",
		"file:../../styles/quantity-selector.css",
		"file:./style-index.css"
	],
	"viewScriptModule": ["@surecart/cart"]
}
