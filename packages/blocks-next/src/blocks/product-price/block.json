{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "surecart/product-price-v2",
	"title": "Price",
	"description": "Display the product price.",
	"version": "1.0.0",
	"category": "surecart",
	"icon": "media-interactive",
	"example": {},
	"usesContext": ["surecart/productId"],
	"ancestor": ["surecart/product-list"],
	"attributes": {
		"show_range": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"interactivity": true,
		"spacing": {
			"padding": true,
			"margin": ["top", "bottom"]
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"__experimentalFontWeight": true
		},
		"color": {
			"text": true,
			"background": false,
			"enableContrastChecker": true
		}
	},
	"textdomain": "surecart",
	"editorScript": "file:./index.js",
	"style": "file:./style-index.css",
	"render": "file:./view.php"
}
