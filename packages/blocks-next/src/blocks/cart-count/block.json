			"lineHeight": true,
			"__experimentalFontWeight": true
		},
		"__experimentalBorder": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"color": {
			"text": true,
			"background": true,
			"enableContrastChecker": true
		}
