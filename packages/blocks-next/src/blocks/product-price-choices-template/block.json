{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "surecart/product-price-choices-template",
	"title": "Prices Template",
	"description": "Displays possible choices for product prices.",
	"version": "1.0.0",
	"category": "surecart-product-page",
	"icon": "media-interactive",
	"usesContext": ["surecart/productId", "surecart/product"],
	"ancestor": ["surecart/product-price-choices-v2"],
	"example": {
		"attributes": {}
	},
	"supports": {
		"interactivity": true,
		"layout": {
			"__experimentalDefault": "grid",
			"__experimentalDefaultControls": {
				"layout": true
			}
		},
		"spacing": {
			"padding": true,
			"margin": false,
			"blockGap": true,
			"__experimentalDefaultControls": {
				"padding": true,
				"blockGap": true
			}
		},
		"__experimentalBorder": {
			"color": true,
			"radius": true,
			"width": true,
			"__experimentalDefaultControls": {
				"color": true,
				"radius": true,
				"width": true
			}
		},
		"color": {
			"gradients": true,
			"text": true,
			"background": true,
			"__experimentalDefaultControls": {
				"text": true,
				"background": true
			}
		}
	},
	"textdomain": "surecart",
	"render": "file:./view.php",
	"editorScript": "file:./index.js",
	"style": ["file:../../styles/choice.css", "file:./style-index.css"]
}
