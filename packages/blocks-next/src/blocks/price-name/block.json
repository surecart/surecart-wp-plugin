{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "surecart/price-name",
	"title": "Price Name",
	"description": "Displays the name of a price.",
	"version": "1.0.0",
	"category": "widgets",
	"icon": "media-interactive",
	"usesContext": ["surecart/priceId", "surecart/price"],
	"ancestor": ["surecart/product-price-choice-template"],
	"supports": {
		"interactivity": true,
		"color": {
			"gradients": true,
			"text": true,
			"background": true,
			"__experimentalDefaultControls": {
				"text": true,
				"background": true
			}
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"textAlign": true,
			"__experimentalFontFamily": true,
			"__experimentalFontWeight": true,
			"__experimentalFontStyle": true,
			"__experimentalTextTransform": true,
			"__experimentalTextDecoration": true,
			"__experimentalLetterSpacing": true,
			"__experimentalDefaultControls": {
				"fontSize": true,
				"fontAppearance": true,
				"textTransform": true
			}
		}
	},
	"textdomain": "surecart",
	"render": "file:./view.php",
	"editorScript": "file:./index.js",
	"style": ["file:../../styles/choice.css"]
}
