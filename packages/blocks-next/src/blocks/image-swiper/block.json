{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "surecart/image-swiper",
	"title": "Image Swiper",
	"description": "Displays the image swiper",
	"version": "1.0.0",
	"category": "widgets",
	"icon": "media-interactive",
	"supports": {
		"interactivity": true,
		"color": {
			"gradients": true,
			"text": true,
			"background": true,
			"__experimentalDefaultControls": {
				"text": true,
				"background": true
			}
		}
	},
	"textdomain": "surecart",
	"render": "file:./view.php",
	"editorScript": "file:./index.js",
	"viewScriptModule": "@surecart/image-slider",
	"style": ["file:../../styles/image-slider.css"]
}
