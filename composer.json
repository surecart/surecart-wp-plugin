{
	"version": "0.16.2",
	"type": "wordpress-plugin",
	"keywords": [
		"wordpress",
		"plugin",
		"starter plugin",
		"wpemerge"
	],
	"license": "GPL-2.0-only",
	"require": {
		"php": ">=5.5",
		"composer/installers": "^1.4",
		"htmlburger/wpemerge": "~0.16.0",
		"htmlburger/wpemerge-app-core": "~0.16.1",
		"htmlburger/wpemerge-blade": "^0.16.1"
	},
	"require-dev": {
		"filp/whoops": "^2.2",
		"htmlburger/wpemerge-cli": "~0.16.0",
		"wp-phpunit/wp-phpunit": "5.7.1",
		"wp-cli/wp-cli-bundle": "*",
		"mockery/mockery": "^1.4",
		"symfony/debug": "~3.0",
		"wp-coding-standards/wpcs": "^2.0",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7.0"
	},
	"autoload": {
		"psr-4": {
			"CheckoutEngine\\": "app/src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CheckoutEngine\\Tests\\": "tests/"
		}
	},
	"scripts": {
		"install-test-env": [
			"wpemerge config:create"
		],
		"install-dev-env": [
			"@php wpemerge config:create",
			"@php wpemerge install:dependencies",
			"@php wpemerge assets:build"
		],
		"post-create-project-cmd": "@install-dev-env"
	}
}
